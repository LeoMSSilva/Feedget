  "devDependencies": {
    "@biomejs/biome": "^1.8.2",
    "@tailwindcss/forms": "^0.5.7",
    "@types/axios": "^0.14.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "autoprefixer": "^10.4.19",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
